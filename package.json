{
	"name": "members-only",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"start": "node ./bin/www",
		"dev": "nodemon ./bin/www",
		"dev:debug": "DEBUG=members-only:* npm run dev",
		"lint": "eslint . --ext js --report-unused-disable-directives --max-warnings 0",
		"lint:fix": "eslint --fix . --ext js --report-unused-disable-directives --max-warnings 0",
		"format": "npx prettier . --check --plugin=@prettier/plugin-pug",
		"format:fix": "npx prettier . --write -plugin=@prettier/plugin-pug",
		"tailwind": "postcss public/stylesheets/tailwind.css -o public/stylesheets/style.css",
		"tailwind:watch": "postcss public/stylesheets/tailwind.css -o public/stylesheets/style.css --watch"
	},
	"dependencies": {
		"autoprefixer": "^10.4.16",
		"cookie-parser": "~1.4.4",
		"debug": "~2.6.9",
		"express": "^4.18.2",
		"http-errors": "~1.6.3",
		"morgan": "~1.9.1",
		"postcss": "^8.4.31",
		"postcss-cli": "^10.1.0",
		"pug": "^3.0.2",
		"tailwindcss": "^3.3.3"
	},
	"devDependencies": {
		"@prettier/plugin-pug": "^3.0.0",
		"eslint": "^8.50.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-config-standard": "^17.1.0",
		"eslint-plugin-import": "^2.28.1",
		"eslint-plugin-n": "^16.1.0",
		"eslint-plugin-prettier": "^5.0.0",
		"eslint-plugin-promise": "^6.1.1",
		"nodemon": "^3.0.1",
		"prettier": "^3.0.3"
	}
}
